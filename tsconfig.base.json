{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@private-prompts/shared/*": [
        "./shared/src/*"
      ]
    },
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "node",
      "vue"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}